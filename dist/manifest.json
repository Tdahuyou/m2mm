{
    "manifest_version": 3,
    "name": "m2mm",
    "version": "1.0.1",
    "description": "Chrome extension to convert GitHub articles or clipboard content into Markmap or Mindmap",
    "background": {
      "service_worker": "service-worker.js"
    },
    "permissions": [
      "sidePanel",
      "clipboardRead"
    ],
    "side_panel": {
      "default_path": "index.html"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "m2mm.png",
        "48": "m2mm.png",
        "192": "m2mm.png"
      },
      "default_title": "Click to open panel"
    }
}